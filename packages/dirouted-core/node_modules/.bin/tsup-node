#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/luc.le/Code/03-lab/dirouted/node_modules/.pnpm/tsup@8.2.3_@swc+core@1.7.3_postcss@8.4.40_typescript@5.5.4/node_modules/tsup/dist/node_modules:/Users/luc.le/Code/03-lab/dirouted/node_modules/.pnpm/tsup@8.2.3_@swc+core@1.7.3_postcss@8.4.40_typescript@5.5.4/node_modules/tsup/node_modules:/Users/luc.le/Code/03-lab/dirouted/node_modules/.pnpm/tsup@8.2.3_@swc+core@1.7.3_postcss@8.4.40_typescript@5.5.4/node_modules:/Users/luc.le/Code/03-lab/dirouted/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/luc.le/Code/03-lab/dirouted/node_modules/.pnpm/tsup@8.2.3_@swc+core@1.7.3_postcss@8.4.40_typescript@5.5.4/node_modules/tsup/dist/node_modules:/Users/luc.le/Code/03-lab/dirouted/node_modules/.pnpm/tsup@8.2.3_@swc+core@1.7.3_postcss@8.4.40_typescript@5.5.4/node_modules/tsup/node_modules:/Users/luc.le/Code/03-lab/dirouted/node_modules/.pnpm/tsup@8.2.3_@swc+core@1.7.3_postcss@8.4.40_typescript@5.5.4/node_modules:/Users/luc.le/Code/03-lab/dirouted/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsup/dist/cli-node.js" "$@"
else
  exec node  "$basedir/../tsup/dist/cli-node.js" "$@"
fi
